h1>Carrito de compras</h1>
<ul>
    {{#each cart.products}}
    <li>
        <h3>{{this.product.title}}</h3>
        <p>{{this.product.description}}</p>
        <p>Precio: ${{this.product.price}}</p>
        <p>Cantidad: {{this.quantity}}</p>
    </li>
    {{/each}}
</ul>

<a href="/products">Volver a la lista de productos</a>